<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Drink_Book_Data_Entry_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Drink_Book_Data_Entry_App"
    xmlns:views="clr-namespace:Drink_Book_Data_Entry_App.Views"
    Shell.FlyoutBehavior="Disabled">
    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="Data"
            ContentTemplate="{DataTemplate views:EntryView}"
            Route="DataView" />

    </TabBar>


</Shell>
